/* pages/artists/artists.wxss */
.head {
    width: 100rpx;
    height: 100rpx;
    float: left;
    line-height: 100rpx;
    text-align: center;
    border-radius: 50%;
    font-size: 1.5em;
    color: white;
    margin-top: 10rpx;
}

navigator.list:last-child view.list-name{
    border-bottom: none;
}

view.list-name{
    margin-left: 120rpx;
    height: 100%;
    line-height: 120rpx;
    border-bottom: 1px solid #DDDDDD;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}