/* pages/sheetInfo/sheetInfo.wxss */
#coverImg {
    height: 8em;
    width: 8em;
    background-color: whitesmoke;
    border-radius: 2px;
    float: left;
    position: relative;
}

.canChange::after{
    content: '';
    position: absolute;
    top: 35%;
    left: 35%;
    height: 30%;
    width: 30%;
    background-size: cover;
    background-image: url('data:image/png;base64,')
}

#btns {
    height: 4em;
    border-top: 1px solid #DDDDDD;
    line-height: 4em;
    display: grid;
    grid-template-columns: 1fr 1fr;
}

.playBtn {
    color: #EA445A;
    width: 95%;
    height: 2.8em;
    line-height: 1.8em;
    font-weight: 600;
}
.weui-dialog {
    top: 35%;
}