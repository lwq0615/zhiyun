/* pages/albumInfo/albumInfo.wxss */

.nav {
    font-size: 1.3em;
    line-height: 1.3em;
    position: fixed;
    top: 0;
    right: 0;
    left: 0;
    z-index: 1;
    background-color: white;
    padding: 0.5em 0.5em 0.3em 0.5em;
}

#album {
    display: grid;
    grid-template-columns: 1fr 1fr 1fr;
    width: 100%;
    grid-row-gap: 0.3em;
    justify-items: center;
    margin-top: 3em;
}

#image {
    width: 8em;
    height: 8em;
    position: relative;
}

#image>image {
    width: 100%;
    height: 100%;
}

/* //伪元素用于照片选中状态 */
.checked::after {
    content: '';
    position: absolute;
    top: 70%;
    left: 70%;
    height: 25%;
    width: 25%;
    background-size: cover;
    background-image: url('data:image/png;base64,');
}

#count {
    text-align: center;
    margin: 10px auto 20px auto;
    font-size: 1.1em;
}

#bigImg {
    position: absolute;
    top: 45%;
    left: 0;
    transform: translateY(-50%);
    height: 100%;
    width: 100%;
    background-color: rgba(0, 0, 0, 0.3);
}

.weui-dialog {
    top: 35%;
}